{
  "name": "@admin-dashboard/server",
  "private": true,
  "type": "module",
  "keywords": [
    "typescript",
    "nestjs"
  ],
  "engines": {
    "node": "23.11.x",
    "npm": "10.9.x"
  },
  "packageManager": "pnpm@10.12.1",
  "main": "dist/main.js",
  "scripts": {
    "build": "node --run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "nest start --watch --debug=0.0.0.0:9229",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts",
    "lint:fix": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts --fix",
    "lint:file": "eslint --ignore-path .gitignore",
    "postinstall": "npx prisma generate && npx prisma migrate deploy"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-fastify": "^11.1.3",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "server": "^1.0.41",
    "typescript-eslint": "8.34.0",
    "@admin-dashboard/shared": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
