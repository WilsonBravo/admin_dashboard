    "prisma": "^6.9.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "unplugin-swc": "^1.5.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.43.0",
    "@swc/core-linux-arm64-musl": "^1.12.0",
    "@swc/core-linux-x64-gnu": "^1.12.0",
    "@swc/core-linux-x64-musl": "^1.12.0"
  }
